# example1.cc

# compile using
c++ -std=c++11 -I/home/avcopan/Documents/code/github/ambit/include -o example1.o -c example1.cc

# link using
c++ -std=c++11 example1.o -o example1.bin /home/avcopan/Documents/code/github/ambit/build/src/libambit.so

# note: must add /path/to/ambit/build/src/ to LD_LIBRARY_PATH
